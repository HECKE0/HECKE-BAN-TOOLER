repeat task.wait() until game:IsLoaded() local lib = { ['notification'] = loadstring(game:HttpGet(("https://raw.githubusercontent.com/HECKE0/HECKE-SCRIPT-FOR-BAN-TOOLER/main/SCRIPT%20FOR%20BAN%20TOOLER"), true))(), ['cooldown'] = false, ['username'] = 'unknown', ['bw'] = 'unknown' } local words = { ['Pula'] = 'Bullying', ['PULA'] = 'Bullying', ['lgbt'] = 'Bullying', ['lesbian'] = 'Bullying', ['gay'] = 'Bullying', ['sperma'] = 'Swearing', ['spermă'] = 'Bullying', ['p|zd@'] = 'Swearing', ['penis'] = 'Swearing', ['morti'] = 'Bullying', ['morți'] = 'Bullying', ['puta'] = 'Swearing', ['puță'] = 'Bullying', ['pisat'] = 'Bullying', ['pișat'] = 'Bullying', ['murata'] = 'Scamming', ['murată'] = 'Bullying', ['pulă'] = 'Bullying', ['curva'] = 'Bullying', ['curvă'] = 'Swearing', ['dracu'] = 'Bullying', ['drk'] = 'Bullying', ['drq'] = 'Bullying', ['prost'] = 'Bullying', ['idiot'] = 'Bullying', ['pizdoi'] = 'Bullying', ['mata'] = 'Bullying', ['măta'] = 'Bullying', ['tacto'] = 'Bullying', ['futut'] = 'Bullying', ['cur'] = 'Bullying', ['Centura'] = 'Bullying', ['locul tau nu e aici'] = 'Bullying', ['locul tău nu e aici'] = 'Bullying', ['adoptat'] = 'Bullying', ['Parasit'] = 'Bullying', ['Părăsit'] = 'Bullying', ['Parașută'] = 'Bullying', ['Parasuta'] = 'Bullying', ['Zdrenta'] = 'Bullying', ['Zdrență'] = 'Bullying', ['copac'] = 'Bullying', ['copil'] = 'Bullying', ['Băgamiaș'] = 'Bullying', ['Bagamias'] = 'Bullying', ['Date'] = 'Swearing', ['Dăte'] = 'Bullying', ['mother'] = 'Bullying', ['Cristomatia'] = 'Bullying', ['Mati'] = 'Bullying', ['Măti'] = 'Bullying', ['Spart'] = 'Scamming', ['Te ia mama'] = 'Scamming', ['retard'] = 'Scamming', ['handicapat'] = 'Offsite Links', ['fututen'] = 'Offsite Links' } local players =
game:GetService('Players') local user = game:GetService('Players').LocalPlayer function lib.notify() lib.notification:message{ Title = "AutoReport", Description = "Reported " .. lib.username .. ' for saying "' .. lib.bw .. '"', Icon = 6023426926 } end function lib.report(user, name, rs) if user and lib.cooldown == false then lib.username = name local suc, er = pcall(function() players:ReportAbuse(players:FindFirstChild(name), rs, 'breaking TOS') end) if not suc then return warn("Couldn't report due to the reason: " .. er .. ' | AR') else lib.notify() end lib.cooldown = true task.wait(5) lib.username = 'unknown' lib.bw = 'unknown' lib.cooldown = false end end players.PlayerChatted:Connect(function(chatType, plr, msg) msg = string.lower(msg) if chatType ~= Enum.PlayerChatType.Whisper and plr ~= user then for i, v in next, words do if string.find(msg, i) then lib.bw = i lib.report(plr.UserId, plr.Name,v) end end end end) lib.notification:message{ Title = "AutoReport", Description = "loaded", Icon = 6023426926 }

